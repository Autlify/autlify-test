{
  "name": "@autlify/billing-sdk",
  "version": "0.5.0",
  "private": true,
  "description": "Premium billing, subscription, and payment gateway SDK for React applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./gateway": {
      "types": "./dist/gateway/index.d.ts",
      "import": "./dist/gateway/index.mjs",
      "require": "./dist/gateway/index.js"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "import": "./dist/registry.mjs",
      "require": "./dist/registry.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js"
    }
  },
  "bin": {
    "billing-sdk": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "echo 'Package is private' && exit 1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-separator": "^1.0.3",
    "@stripe/stripe-js": "^3.0.0",
    "@stripe/react-stripe-js": "^2.6.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.344.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "eslint": "^8.0.0",
    "tailwindcss": "^3.4.0"
  },
  "keywords": [
    "react",
    "billing",
    "subscription",
    "stripe",
    "payment",
    "payment-gateway",
    "paykit",
    "checkout",
    "components",
    "ui",
    "nextjs",
    "tailwindcss",
    "autlify"
  ],
  "author": "Autlify",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/autlify/autlify"
  },
  "bugs": {
    "url": "https://github.com/autlify/autlify/issues"
  },
  "homepage": "https://naropo.com/docs/billing-sdk"
}